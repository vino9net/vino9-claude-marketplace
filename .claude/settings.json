{
  "permissions": {
    "allow": [
      "WebSearch",
      "WebFetch(domain:*)",
      "WebFetch(domain:github.com)",
      "WebFetch(domain:*.githubusercontent.com)",
      "Bash(ls:*)",
      "Bash(cp:*)",
      "Bash(mv:*)",
      "Bash(rm:*)",
      "Bash(mkdir:*)",
      "Bash(rg:*)",
      "Bash(grep:*)",
      "Bash(find:*)",
      "Bash(git add:*)",
      "Bash(git log:*)",
      "Bash(git mv:*)",
      "Bash(git worktree:*)",
      "Bash(cat:*)",
      "Read(/tmp/claude/*)"
    ],
    "ask": [
      "Bash(git commit:*)",
      "Bash(git push:*)"
    ],
    "deny": [],
    "defaultMode": "acceptEdits"
  },
  "includeCoAuthoredBy": false
}
